<view class="tubiao data-v-2625240c"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item})}}" class="goods-item data-v-2625240c" bindtap="__e"><view class="image data-v-2625240c"><image src="{{item.mainPic}}" class="data-v-2625240c"></image><block wx:if="{{item.shopType==0}}"><text class="tag data-v-2625240c">淘宝</text></block><block wx:else><text class="tag data-v-2625240c">天猫</text></block></view><view class="content data-v-2625240c"><view class="title data-v-2625240c">{{''+item.title+''}}</view><view class="sales data-v-2625240c"><text class="data-v-2625240c">{{item.shopName}}</text><text class="data-v-2625240c">{{"月销"+item.monthSales+"件"}}</text></view><view class="Coupon data-v-2625240c"><text class="data-v-2625240c">{{"券后"+item.actualPrice}}</text><button data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" data-event-params="{{({item})}}" catchtap="__e" class="data-v-2625240c">{{"领"+item.couponPrice+"元券"}}</button></view></view></view></block></view>